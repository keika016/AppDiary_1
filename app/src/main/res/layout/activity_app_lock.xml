<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.son_auto.appdiary_1.AppLockActivity">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Set App Lock" />

    <EditText
        android:id="@+id/activity_applock_editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name" />

    <Button
        android:id="@+id/activity_applock_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Set App Lock" />

    <Button
        android:id="@+id/activity_applock_button_delete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Delete App Lock" />
</LinearLayout>
